


# === Custom rules v1.5 ===
<IfModule mod_rewrite.c>
RewriteEngine On
# Serve the external PNG favicon for any /favicon.ico request
RewriteCond %{REQUEST_URI} ^/favicon\.ico$ [NC]
RewriteRule ^favicon\.ico$ https://insat.com.ar/img/favicon.png [R=302,L]
</IfModule>
# === End custom rules v1.5 ===


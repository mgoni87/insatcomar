# BEGIN Block WordPress xmlrpc.php requests
<Files "xmlrpc.php">
order deny,allow
deny from all
allow from 127.0.0.1
#allow from XXX.XXX.XXX.XXX
</Files>
# END Block WordPress xmlrpc.php requests

# BEGIN Restrict good Web Crawlers/User-Agent to a certain time range
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteCond %{TIME_HOUR}%{TIME_MIN} >0800
RewriteCond %{TIME_HOUR}%{TIME_MIN} <2000
# Top 10 Web Crawlers
RewriteCond %{HTTP_USER_AGENT} ^.*(Googlebot|Bingbot|Slurp|DuckDuckBot|Baiduspider|YandexBot|Sogou|Exabot|FacebookBot|meta-externalagent|Applebot|Bytespider|Amazonbot|GPTBot).*$ [NC]
RewriteRule .* - [R=503,L]
</IfModule>
# END Restrict good Web Crawlers/User-Agent to a certain time range

# BEGIN Block Tools and Libraries/User-Agent
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteCond %{HTTP_USER_AGENT} ^.*(curl|wget|sqlmap|WPScan|JoomlaScan|Acunetix|CensysInspect|FHscan|HTTrack|libwww-perl|urllib|python-requests|python-httplib2|winhttp|lwp-request|lwp-trivial|fasthttp|Go-http-client|Java|httplib|httpclient|Zend_Http_Client|Apache-HttpClient).*$ [NC]
RewriteRule .* - [R=503,L]
</IfModule>
# END Block Tools and Libraries/User-Agent

# BEGIN Block bad User-Agent
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteCond %{HTTP_USER_AGENT} ^.*(SemrushBot|AhrefsBot|Barkrowler|UCBrowser|MQQBrowser|LieBaoFast|Mb2345Browser|serpstatbot|Linguee|PetalBot|Seekport|SeekportBot|DotBot|PetalBot|trendictionbot|DataForSeoBot|Semanticbot|Buck|Barkrowler|ips-agent|GeedoBot|MJ12bot|GrapeshotCrawler|heritrix|OpenindexSpider|MetadataScraper|Verity|proximic|serpstatbot|AwarioBot|ClaudeBot|Paqlebot|SentiBot|Friendly_Crawler|BLEXBot|GeedoProductSearch|ByteSpider|Bytedance|SentiBot|MuckRack|ImagesiftBot|cortex|wine-searcherbot).*$ [NC]
RewriteRule .* - [R=503,L]
</IfModule>
# END Block bad User-Agent

# BEGIN WordPress
# Las directivas (líneas) entre «BEGIN WordPress» y «END WordPress» son
# generadas dinámicamente y solo deberían ser modificadas mediante filtros de WordPress.
# Cualquier cambio en las directivas que hay entre esos marcadores serán sobrescritas.
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>

# END WordPress

# BEGIN WP-HUMMINGBIRD-CACHING
# Las directivas (líneas) entre «BEGIN WP-HUMMINGBIRD-CACHING» y «END WP-HUMMINGBIRD-CACHING» son
# generadas dinámicamente y solo deberían ser modificadas mediante filtros de WordPress.
# Cualquier cambio en las directivas que hay entre esos marcadores serán sobrescritas.
<IfModule mod_expires.c>
ExpiresActive On
ExpiresDefault A0

<FilesMatch "\.(txt|xml|js)$">
ExpiresDefault A31536000
</FilesMatch>

<FilesMatch "\.(css)$">
ExpiresDefault A31536000
</FilesMatch>

<FilesMatch "\.(flv|ico|pdf|avi|mov|ppt|doc|mp3|wmv|wav|mp4|m4v|ogg|webm|aac|eot|ttf|otf|woff|woff2|svg)$">
ExpiresDefault A31536000
</FilesMatch>

<FilesMatch "\.(jpg|jpeg|png|gif|swf|webp)$">
ExpiresDefault A31536000
</FilesMatch>
</IfModule>

<IfModule mod_headers.c>
<FilesMatch "\.(txt|xml|js)$">
Header set Cache-Control "max-age=31536000"
</FilesMatch>

<FilesMatch "\.(css)$">
Header set Cache-Control "max-age=31536000"
</FilesMatch>

<FilesMatch "\.(flv|ico|pdf|avi|mov|ppt|doc|mp3|wmv|wav|mp4|m4v|ogg|webm|aac|eot|ttf|otf|woff|woff2|svg)$">
Header set Cache-Control "max-age=31536000"
</FilesMatch>

<FilesMatch "\.(jpg|jpeg|png|gif|swf|webp)$">
Header set Cache-Control "max-age=31536000"
</FilesMatch>
</IfModule>
# END WP-HUMMINGBIRD-CACHING
# BEGIN WP-HUMMINGBIRD-GZIP
# Las directivas (líneas) entre «BEGIN WP-HUMMINGBIRD-GZIP» y «END WP-HUMMINGBIRD-GZIP» son
# generadas dinámicamente y solo deberían ser modificadas mediante filtros de WordPress.
# Cualquier cambio en las directivas que hay entre esos marcadores serán sobrescritas.
<IfModule mod_deflate.c>
	SetOutputFilter DEFLATE
    <IfModule mod_setenvif.c>
        <IfModule mod_headers.c>
            SetEnvIfNoCase ^(Accept-EncodXng|X-cept-Encoding|X{15}|~{15}|-{15})$ ^((gzip|deflate)\s*,?\s*)+|[X~-]{4,13}$ HAVE_Accept-Encoding
            RequestHeader append Accept-Encoding "gzip,deflate" env=HAVE_Accept-Encoding
        </IfModule>
    </IfModule>
    <IfModule mod_filter.c>
        AddOutputFilterByType DEFLATE "application/atom+xml" \
                                      "application/javascript" \
                                      "application/json" \
                                      "application/ld+json" \
                                      "application/manifest+json" \
                                      "application/rdf+xml" \
                                      "application/rss+xml" \
                                      "application/schema+json" \
                                      "application/vnd.geo+json" \
                                      "application/vnd.ms-fontobject" \
                                      "application/x-font-ttf" \
                                      "application/x-font-opentype" \
                                      "application/x-font-truetype" \
                                      "application/x-javascript" \
                                      "application/x-web-app-manifest+json" \
                                      "application/xhtml+xml" \
                                      "application/xml" \
                                      "font/eot" \
                                      "font/opentype" \
                                      "font/otf" \
                                      "image/bmp" \
                                      "image/svg+xml" \
                                      "image/vnd.microsoft.icon" \
                                      "image/x-icon" \
                                      "text/cache-manifest" \
                                      "text/css" \
                                      "text/html" \
                                      "text/javascript" \
                                      "text/plain" \
                                      "text/vcard" \
                                      "text/vnd.rim.location.xloc" \
                                      "text/vtt" \
                                      "text/x-component" \
                                      "text/x-cross-domain-policy" \
                                      "text/xml"

    </IfModule>
    <IfModule mod_mime.c>
        AddEncoding gzip              svgz
    </IfModule>
</IfModule>
# END WP-HUMMINGBIRD-GZIP

## WP Defender - Prevent information disclosure ##

<FilesMatch "\.(md|exe|sh|bak|inc|pot|po|mo|log|sql)$">
Order allow,deny
Deny from all
</FilesMatch>

<Files robots.txt>
Allow from all
</Files>

<Files ads.txt>
Allow from all
</Files>

## WP Defender - End


# BEGIN SMUSH-WEBP
# Las directivas (líneas) entre «BEGIN SMUSH-WEBP» y «END SMUSH-WEBP» son
# generadas dinámicamente y solo deberían ser modificadas mediante filtros de WordPress.
# Cualquier cambio en las directivas que hay entre esos marcadores serán sobrescritas.
<IfModule mod_rewrite.c>
 RewriteEngine On
 RewriteCond %{DOCUMENT_ROOT}/wp-content/smush-webp/disable_smush_webp !-f
 RewriteCond %{HTTP_ACCEPT} image/webp
 RewriteCond %{DOCUMENT_ROOT}/wp-content/smush-webp/$1.webp -f
 RewriteRule wp-content/uploads/(.*\.(?:png|jpe?g))$ wp-content/smush-webp/$1.webp [NC,T=image/webp]
</IfModule>

<IfModule mod_headers.c>
 Header append Vary Accept env=WEBP_image
</IfModule>

<IfModule mod_mime.c>
 AddType image/webp .webp
</IfModule>
# END SMUSH-WEBP

